{
  "name": "jalexa",
  "version": "1.0.0",
  "description": "Easily test alexa skills.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ben Force <benforce@gmail.com>",
  "bugs": "https://github.com/theBenForce/jalexa/issues",
  "homepage": "https://github.com/theBenForce/jalexa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:theBenForce/jalexa.git"
  },
  "dependencies": {
    "ask-cli": "^2.22.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "tsc"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "alexa",
    "testing",
    "jest"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# jalexa Change Log",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "verifyConditions": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog"
    ],
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": "CHANGELOG.md"
      }
    ]
  }
}
