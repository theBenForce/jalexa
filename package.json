{
  "name": "jalexa",
  "version": "1.0.0",
  "description": "Easily test alexa skills.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ben Force <benforce@gmail.com>",
  "bugs": "https://github.com/theBenForce/jalexa/issues",
  "homepage": "https://github.com/theBenForce/jalexa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:theBenForce/jalexa.git"
  },
  "dependencies": {
    "ask-cli": "^2.20.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "tsc"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "alexa",
    "testing",
    "jest"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# jalexa Change Log",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "verifyConditions": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog"
    ],
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": "CHANGELOG.md"
      }
    ]
  }
}
